// Geometry parameters
f 0.01;   // z-front
b 0;      // z-back

x0 -5.5;  // front layer
x1 -2.0;  // front frame square pillar
x2 -1.0;  // front square pillar
x3  0.0;  // back square pillar
x4  1.0;  // back frame square pillar
x5  4.0;  // back beam
x6  14.0; // back layer

y0  -6;   // bottom
y1   6;   // top

// Grid refinement parameters
X1 17;	// Nx of 0th block,0.5
X2 22;	// Nx of 3rd block,0.06
X3 12;	// Nx of 8th block,0.06
X4 24;	// Nx of 10th block,0.06
X5 40;	// Nx of 14th block,0.194

GX1 0.5; // Grading X1
GX3 2;   // Grading X3 (pillar)
IGX3 0.5;// Inverse Grading of GX3 (pillar)
GX5 3.5; // Grading X5

Y1 20;	// Ny of 0th block,0.5
Y2 10;  // Ny of 8th block,0.06
Y3 1;		// Ny of 18th block,0.06

GY1 2;  // Grading of Y1
IGY1 0.5; // Inverse grading of Y1

